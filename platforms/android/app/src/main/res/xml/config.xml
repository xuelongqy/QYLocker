<?xml version='1.0' encoding='utf-8'?>
<widget id="com.qingyi.applocker" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="StatusBarAndNavigationBar">
        <param name="android-package" value="com.qingyi.applocker.cordova.plugin.StatusBarAndNavigationBarPlugin" />
    </feature>
    <feature name="Toast">
        <param name="android-package" value="com.qingyi.applocker.cordova.plugin.ToastPlugin" />
    </feature>
    <feature name="Logger">
        <param name="android-package" value="com.qingyi.applocker.cordova.plugin.LoggerPlugin" />
    </feature>
    <feature name="LockApps">
        <param name="android-package" value="com.qingyi.applocker.cordova.plugin.LockAppsPlugin" />
    </feature>
    <name>QYLocker</name>
    <description>
        An app lock that allows your application lock interface to look anything like it.
    </description>
    <author email="xuelongqy@foxmail.com" href="http://www.xuelongqy.top">
        qingyi
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <feature name="Keyboard">
        <param name="android-package" onload="true" value="io.ionic.keyboard.IonicKeyboard" />
    </feature>
</widget>
